{
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++20 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt < $dir/input_file.in > $dir/output_file.out",
    },
    "files.exclude": {
        "**/*.exe": true,
        "**/*.out": true,
        "**/*.o": true,
        "**/*.obj": true,
        "**/*.dSYM": true
    }
}